<?xml version="1.0" encoding="utf-8"?>
<config>
	<fieldset name="module">
		<field
			name="collect_feedback"
			type="radio"
			default="1"
			label="knowledge::knowledge.params.collect feedback"
			description="knowledge::knowledge.params.collect feedback desc">
			<option value="0">global.no</option>
			<option value="1">global.yes</option>
		</field>
	</fieldset>
	<fieldset
		name="permissions"
		label="access.permissions"
		description="access.permissions desc"
		>

		<field
			name="rules"
			type="rules"
			label="access.permissions"
			filter="rules"
			validate="rules"
			module="knowledge"
			section="module" />
	</fieldset>
</config>
